<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>

<addon id="repository.brazilian.zararepo"
        name="[COLOR white][B]Zara Repo[/B][/COLOR]"
        version="1.0.3"
        provider-name="Zara">
    <extension point="xbmc.addon.repository" name="Brazilian XBMC Add-on Repository">
        <info compressed="false">http://raw.githubusercontent.com/ZaraSeries/repo/master/addons.xml</info>
        <checksum>http://raw.githubusercontent.com/ZaraSeries/repo/master/addons.xml.md5</checksum>
        <datadir zip="true">http://raw.githubusercontent.com/ZaraSeries/repo/master/repo</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>[COLOR white][B]Zara Repo[/B][/COLOR]</summary>
        <description>O melhor repositorio BR</description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="plugin.video.zaraseries"
       name="[COLOR white][B]Zara[/B][/COLOR] [COLOR cyan][B]Series[/B][/COLOR]"
       version="1.0.1"
       provider-name="Zara Series">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" /> 
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>

  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">[COLOR white][B]Zara[/B][/COLOR] [COLOR cyan][B]Series[/B][/COLOR]</summary>
    <description lang="en">As melhores series você encontra aqui!</description>
    <platform>all</platform>
  </extension>
</addon>

